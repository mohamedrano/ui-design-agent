# Google AI Configuration
GOOGLE_GENAI_API_KEY=your_google_ai_api_key_here

# Redis Configuration (Optional - for rate limiting and caching)
UPSTASH_REDIS_REST_URL=your_upstash_redis_url_here
UPSTASH_REDIS_REST_TOKEN=your_upstash_redis_token_here

# Sentry Configuration (Optional - for error tracking)
SENTRY_DSN=your_sentry_dsn_here
SENTRY_ORG=your_sentry_org_here
SENTRY_PROJECT=your_sentry_project_here

# Figma Integration (Optional)
FIGMA_ACCESS_TOKEN=your_figma_access_token_here

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# Performance Budgets
LIGHTHOUSE_BUDGET_LCP_DESKTOP=2500
LIGHTHOUSE_BUDGET_LCP_MOBILE=3500
LIGHTHOUSE_BUDGET_BUNDLE_SIZE_KB=300

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=60
RATE_LIMIT_BURST_SIZE=10

# AI Model Configuration
AI_MODEL_TEMPERATURE_ANALYSIS=0.2
AI_MODEL_TEMPERATURE_GENERATION=0.7
AI_MODEL_MAX_TOKENS=4000
AI_MODEL_TIMEOUT_MS=30000
